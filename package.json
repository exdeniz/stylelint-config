{
  "name": "@logux/stylelint-config",
  "version": "0.2.0",
  "description": "Stylelint shareable config for Logux",
  "keywords": [
    "logux",
    "stylelint",
    "stylelint-config"
  ],
  "scripts": {
    "test": "yaspeller *.md"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/stylelint-config",
  "dependencies": {
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0"
  },
  "peerDependencies": {
    "stylelint": "^13.12.0"
  },
  "devDependencies": {
    "clean-publish": "^1.1.9",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.4",
    "stylelint": "^13.12.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "yaspeller": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yaspeller"
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)."
    ],
    "dictionary": [
      "Versioning",
      "Stylelint",
      "Logux"
    ]
  }
}
